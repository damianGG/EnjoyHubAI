# Cloudinary Configuration
# Sign up at https://cloudinary.com and get your credentials from the Dashboard
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# SMS Configuration for Phone Authentication
# Note: Supabase handles SMS sending through configured providers
# Configure in Supabase Dashboard > Authentication > Phone Auth
# Supported providers: Twilio, MessageBird, Textlocal, Vonage
# For SMSApi.pl integration, you may need to use a custom provider or webhook
NEXT_PUBLIC_ENABLE_PHONE_AUTH=true
